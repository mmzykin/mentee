[run]
source = .
omit = 
    tests/*
    scripts/*
    */__pycache__/*
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

show_missing = True
precision = 1

[html]
directory = coverage_html
title = Mentee Bot Coverage Report
